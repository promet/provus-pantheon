{
    "name": "pantheon-upstreams/upstream-configuration",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/admin_toolbar": "^3.0",
        "drupal/antibot": "^1.5",
        "drupal/address": "^1.9",
        "drupal/calendar_link": "^2.1",
        "drupal/chosen": "^3.0",
        "drupal/config_ignore": "^2.3",
        "drupal/components": "^2.4",
        "drupal/devel": "^4.1",
        "drupal/dropzonejs": "^2.5",
        "drupal/emulsify_twig": "^2.0",
        "drupal/entity": "^1.2",
        "drupal/environment_indicator": "^4.0",
        "drupal/entity_embed": "^1.2",
        "drupal/extlink": "^1.6",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.2",
        "drupal/focal_point": "^1.5",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/geocoder": "^3.24",
        "drupal/geofield": "^1.36",
        "drupal/geofield_map": "^2.71",
        "drupal/google_analytics": "^4.0",
        "drupal/layout_builder_at": "^2.12"
